{
    "name": "markosiilak/drupal-pathauto-edit-links",
    "description": "Drupal module that enables node edit links to use pathauto aliases instead of node IDs. Serves edit forms directly at clean URLs like /about/edit.",
    "type": "drupal-module",
    "keywords": [
        "drupal",
        "module",
        "pathauto",
        "edit-links",
        "clean-urls",
        "seo"
    ],
    "homepage": "https://github.com/markosiilak/drupal-pathauto-edit-links",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Marko Siilak",
            "email": "start@siilak.com",
            "homepage": "https://siilak.com"
        }
    ],
    "support": {
        "issues": "https://github.com/markosiilak/drupal-pathauto-edit-links/issues",
        "source": "https://github.com/markosiilak/drupal-pathauto-edit-links"
    },
    "require": {
        "php": ">=8.1",
        "drupal/core": "^10.0 || ^11.0",
        "drupal/pathauto": "^1.11"
    },
    "require-dev": {
        "drupal/core-dev": "^10.0 || ^11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "drupal": {
            "version": "1.0.0",
            "datestamp": "2024-09-18"
        }
    }
}
